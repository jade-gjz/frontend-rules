{
  "name": "jade-frontend-rules",
  "version": "1.0.0",
  "description": "jade前端自定义代码规范",
  "private": true,
  "scripts": {
    "build": "pnpm run build --filter @jhqn/*",
    "prepublishOnly": "pnpm run prepublishOnly --filter @jhqn/*",
    "update": "pnpm up -i -r",
    "update:all": "pnpm up -r",
    "release": "pnpm publish -r --no-git-checks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaderd-jh/frontend-rules.git"
  },
  "keywords": [
    "jade"
  ],
  "author": "jade-gjz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaderd-jh/frontend-rules/issues"
  },
  "homepage": "https://github.com/jaderd-jh/frontend-rules#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.23",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  }
}
